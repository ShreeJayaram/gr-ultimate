<?xml version="1.0"?>
<block>
  <name>orange</name>
  <key>OOKz_orange</key>
  <category>[OOKz]</category>
  <import>import OOKz</import>
  <make>OOKz.orange($interpolation, $amplitude, $offset)</make>
  <callback>set_interpolation($interpolation)</callback> 
  <callback>set_amplitude($amplitude)</callback> 
  <callback>set_offset($offset)</callback> 

  <param>
    <name>interpolation</name>
    <key>interpolation</key>
    <type>float</type>
  </param>


  <param>
    <name>amplitude</name>
    <key>amplitude</key>
    <type>int</type>
  </param> 

 <param>
    <name>offset</name>
    <key>offset</key>
    <type>int</type>
  </param> 

 
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>


  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>

