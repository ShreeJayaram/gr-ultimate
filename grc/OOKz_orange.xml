   <option>
     <name>amplitude</name>
     <key>int</key>
     <opt>fcn:i</opt>
     <opt>amplitude_type:int</opt>
   </option>
  </param>

    <param>
	<name>offset</name>
	<key>offset</key>
	<value>0</value>
	<type>$type.offset_type</type>
    </param>

  <param>
	<name>amplitude</name>
	<key>amplitude</key>
	<value>0</value>
	<type>$type.amplitude_type</type>
    </param>

 
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>


  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>

